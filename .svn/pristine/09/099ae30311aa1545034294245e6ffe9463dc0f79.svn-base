  /**
    * 批量保存预测结果到数据库MySQL，并告警
    *
    * @param resList 预测结果集合
    */
  def saveBatchResult(resList: List[PredResult]) = {
    var pstInsert: PreparedStatement = null
    //仅当传入的预测结果集合不为空时，执行保存动作
    if (resList != null && resList.size != 0) {
      //批量执行SQL的条数
      logger.warn("**** 当前预测结果需保存数量合计 ===> " + resList.size)
      //SQL：向MySQL中的t_pred_result中插入预测结果
      val sqlInsert = "INSERT INTO t_pred_result(sysName,hostName,predIndexName,currentTime,indexTyp,predPeriod,dataAcceptTime,currentDataTime,currentActualValue,nextPredValue,nextPredMinValue,nextPredMaxValue) " +
        "VALUES(?,?,?,?,?,?,?,?,?,?,?,?)"
      var conn: Connection = null
      try {
        conn = ConnPoolUtil2.getConn()
        pstInsert = conn.prepareStatement(sqlInsert)
        for (i <- 0 until resList.size) {
          pstInsert.setString(1, resList(i).getSysName) //系统名称中文名
          pstInsert.setString(2, resList(i).getHostName) //主机名
          pstInsert.setString(3, resList(i).getPredIndexName) //预测指标名称
          pstInsert.setString(4, (System.currentTimeMillis() / 1000).toString) //数据保存时间
          pstInsert.setString(5, resList(i).getIndexTyp) //指标类型：00->单指标,01->复合指标
          pstInsert.setString(6, resList(i).getPredPeriod) //预测周期（分钟）00->5分钟,01->15分钟,02->30分钟,03->60分钟
          pstInsert.setString(7, resList(i).getDataAcceptTime) //数据接收时间，即从kafka中消费到消息的时间
          pstInsert.setString(8, resList(i).getCurrentDataTime) //所预测数据的时刻
          pstInsert.setString(9, resList(i).getCurrentActualValue) //实测值(由于为预测时刻，实际值未到，暂设空)
          pstInsert.setString(10, resList(i).getNextPredValue) //下一时刻预测值
          pstInsert.setString(11, resList(i).getNextPredMinValue) //下一时刻预测下限
          pstInsert.setString(12, resList(i).getNextPredMaxValue) //下一时刻预测上限
          pstInsert.addBatch()
        }
        pstInsert.executeBatch()
      } catch {
        case e: Exception => logger.error("SQL Exception!!!数据库操作异常!!:addBatchPredResult()" + e.getMessage + e.printStackTrace())
      } finally {
        if (conn != null) {
          //释放数据库连接
          ConnPoolUtil2.releaseCon(conn)
        }
        ConnUtils.closeStatement(pstInsert,null , null)
      }
    }
  }






  /**
    * 保存预测结果到数据库（不再使用）
    *
    * @param res  预测结果
    * @param conn 数据库连接
    * @return 返回当前insert的主键
    */
  def addPredResult(res: PredResult, conn: Connection): Integer = {
    var newId = -1
    if (res != null && conn != null && (!conn.isClosed)) {
      val sql1 = "INSERT INTO t_pred_result(sysName,hostName,predIndexName,currentTime,indexTyp,predPeriod,dataAcceptTime,currentDataTime,currentActualValue,nextPredValue,nextPredMinValue,nextPredMaxValue) " +
        "VALUES(?,?,?,?,?,?,?,?,?,?,?,?)"
      val sql2 = "SELECT LAST_INSERT_ID()"
      try {
        pst = conn.prepareStatement(sql1)
        pst.setString(1, res.getSysName)
        pst.setString(2, res.getHostName)
        pst.setString(3, res.getPredIndexName)
        pst.setString(4, new Date().getTime.toString)
        pst.setString(5, res.getIndexTyp)
        pst.setString(6, res.getPredPeriod)
        pst.setString(7, res.getDataAcceptTime)
        pst.setString(8, res.getCurrentDataTime)
        pst.setString(9, res.getCurrentActualValue)
        pst.setString(10, res.getNextPredValue)
        pst.setString(11, res.getNextPredMinValue)
        pst.setString(12, res.getNextPredMaxValue)
        pst.executeUpdate()
        st = conn.createStatement()
        rs = st.executeQuery(sql2)
        if (rs != null && rs.next()) {
          newId = rs.getInt(1)
        }
        conn.commit()
      } catch {
        case e: Exception => logger.error("SQL Exception!!!数据库操作异常!!" + e.printStackTrace())
      } finally {
        ConnUtils.closeStatement(st, pst, rs)
      }
    }
    newId
  }


  /**
    * 保存预测结果到数据库（不再使用）
    *
    * @param res  预测结果
    * @param conn 数据库连接
